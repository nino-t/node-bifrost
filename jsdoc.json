{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": [ "./" ],
    "exclude": [ "node_modules", "docs" ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "templates": {
    "default": {
      "outputSourceFiles": false
    }
  }
}